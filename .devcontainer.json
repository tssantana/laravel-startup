{
  "name": "API Monitor",
  "dockerComposeFile": "docker-compose.yml",
  "service": "api-monitor",
  "remoteUser": "root",
  "workspaceFolder": "/var/www/html/app",
  "customizations": {
    "vscode": {
      "extensions": [
        "eamodio.gitlens",
        "xdebug.php-debug",
        "mikestead.dotenv",
        "felixfbecker.php-debug",
        "onecentlin.laravel-blade",
        "EditorConfig.EditorConfig",
        "fterrag.vscode-php-cs-fixer",
        "onecentlin.laravel5-snippets",
        "bmewburn.vscode-intelephense-client",
        "mehedidracula.php-namespace-resolver",
        "SonarSource.sonarlint-vscode",
        "amiralizadeh9480.laravel-extra-intellisense"
      ],
      "settings": {
        "terminal.integrated.defaultProfile.linux": "bash",
        "namespaceResolver.sortAlphabetically": false,
        "namespaceResolver.autoSort": false,
        "namespaceResolver.sortOnSave": false,
        "namespaceResolver.showMessageOnStatusBar": false,
        "namespaceResolver.exclude": "*/node_modules/*",
        "[php]": {
          "editor.tabSize": 4,
          "editor.defaultFormatter": "bmewburn.vscode-intelephense-client",
          "editor.formatOnSave": true
        },
        "php.validate.executablePath": "/usr/local/bin/php"
      }
    }
  },
  "shutdownAction": "none"
}
